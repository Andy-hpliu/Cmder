clear=cls
ls=ls --color $*

gl=git log -n 35 --pretty=format:"%C(auto) %h %<|(25) %ar %<|(90) %s %d" $*
gdf=git diff --full-index
gd=git diff $*

gh=git help $*
gb=git branch -v $*
gch=git checkout $*

gs=git status $*
ga=git add -A $*
gc=git commit -m $*

gpl=git pull
gp=git push origin master

now=time /t && date /t
