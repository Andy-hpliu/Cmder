clear=cls
ls=ls --color --width=115 -F $*

now=time /t $T date /t

kill=taskkill /f /t /im $*

gl=git log --pretty=format:"%C(auto) %h %<|(25) %ar %<|(90) %s %d" $*
gdf=git diff --full-index
gd=git diff $*

gh=git help $*
gb=git branch -v $*
gch=git checkout $*

gs=git status $*
ga=git add -A $*
gc=git commit -m $*
gce=git commit -e $*

gpl=git pull $*
gp=git push origin master
